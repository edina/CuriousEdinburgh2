{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "indent": ["error", 4],
    "no-underscore-dangle": [2, { "allowAfterThis": true }]
  },
  "env": {
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
